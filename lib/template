#!/bin/bash
git init -q
git remote add origin https://github.com/roblefort/docker_testnegative
git fetch origin -q
git checkout origin/master -- compose
mv compose/* ./
mv compose/.gitignore ./
rm -rf compose .git
git init
mkdir ./magento
cd ./magento 
git clone -b develop git@github.com:Legends-Mkt/testnegative_magento.git .
